{
  "name": "three-cad-viewer",
  "version": "4.0.0",
  "type": "module",
  "description": "WebGL-based CAD viewer built on Three.js with clipping planes, measurement tools, and tree navigation",
  "repository": {
    "type": "git",
    "url": "https://github.com/bernhard-42/three-cad-viewer"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/three-cad-viewer.js",
  "module": "dist/three-cad-viewer.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/three-cad-viewer.esm.js",
      "require": "./dist/three-cad-viewer.js",
      "types": "./dist/index.d.ts"
    },
    "./css": "./dist/three-cad-viewer.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "node ./scripts/copy_version.cjs && rollup -c --environment BUILD:production",
    "prepare": "yarn run build",
    "start": "rollup -w -c",
    "debug": "rollup -w -c --environment BUILD:development --sourcemap",
    "docs": "./scripts/build_docs.sh",
    "release": "./scripts/release.sh",
    "lint": "eslint src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:run-all": "RUN_SLOW_TESTS=true vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "author": "Bernhard Walter",
  "license": "MIT",
  "dependencies": {
    "three": "0.180.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/three": "0.180.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "cross-spawn": "^7.0.6",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "18.0.1",
    "jsdoc": "^4.0.5",
    "postcss": "^8.0.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-import-css": "^4.0.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^3.0.0",
    "rollup-plugin-string": "^3.0.0",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vitest": "3.2.4",
    "yarn": "^1.22.22"
  },
  "babel": {},
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "esbuild": "0.25.9",
    "semver": "7.7.2",
    "glob": "10.4.5",
    "@babel/parser": "7.27.7",
    "@babel/types": "7.27.7",
    "@babel/helper-validator-identifier": "7.27.1",
    "typescript": "5.9.2",
    "@types/three": "0.180.0",
    "@rollup/plugin-typescript": "12.1.4",
    "tslib": "2.8.1",
    "@types/webxr": "0.5.23",
    "@webgpu/types": "0.1.65",
    "@types/stats.js": "0.17.4",
    "fflate": "0.8.2",
    "meshoptimizer": "0.22.0",
    "@tweenjs/tween.js": "23.1.3",
    "@dimforge/rapier3d-compat": "0.12.0",
    "resolve": "1.22.10",
    "@rollup/pluginutils": "5.1.4",
    "picomatch": "4.0.2",
    "@types/estree": "1.0.6",
    "estree-walker": "2.0.2",
    "path-parse": "1.0.7",
    "is-core-module": "2.16.1",
    "supports-preserve-symlinks-flag": "1.0.0",
    "hasown": "2.0.2",
    "function-bind": "1.1.2",
    "@eslint-community/regexpp": "4.12.1"
  }
}
